              <property name="styleSheet">
               <string notr="true">QTableView {
    border: 2px solid #8f8f91;
    border-radius: 6px;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                      stop:0 #f6f7fa, stop:1 #dadbde);
    font: 10pt &quot;Roboto&quot;;
    padding: 5px;
    color: black;
    gridline-color: #8f8f91; /* optional: grid lines color */
}

/* Header sections (horizontal and vertical) */
QHeaderView::section {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                      stop:0 #616161, stop: 0.5 #505050, stop: 0.6 #434343, stop:1 #656565);
    color: white;
    padding-left: 5px;
    border: 1px solid #6c6c6c;
    font: bold 14pt &quot;Segoe UI&quot;;
    height: 30px; /* header height */
    border-radius: 4px;
}

/* Vertical header specific */
QHeaderView:vertical {
    min-width: 40px; /* ensure vertical header is wide enough */
    border-right: 1px solid #6c6c6c;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                      stop:0 #616161, stop: 0.5 #505050, stop: 0.6 #434343, stop:1 #656565);
}

/* Scrollbar background and border */
QTableView QScrollBar:vertical, QTableView QScrollBar:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #f6f7fa, stop:1 #dadbde);
    border: 2px solid #8f8f91;
    border-radius: 6px;
    width: 14px;
    height: 14px;
    margin: 2px 0 2px 0;
}

/* Remove bottom button of vertical scrollbar */
QTableView QScrollBar::add-line:vertical {
    height: 0px;
    width: 0px;
    border: none;
    background: none;
    margin: 0px;
    padding: 0px;
}

/* Scrollbar handle */
QTableView QScrollBar::handle:vertical, QTableView QScrollBar::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #8f8f91, stop:1 #6f6f71);
    min-height: 20px;
    border-radius: 6px;
    border: 1px solid #6f6f71;
}

/* Scrollbar handle hover */
QTableView QScrollBar::handle:vertical:hover, QTableView QScrollBar::handle:horizontal:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #6f6f71, stop:1 #4f4f51);
    border: 1px solid #4f4f51;
}

/* Scrollbar buttons (sub-line and add-line) */
QTableView QScrollBar::sub-line:vertical, QTableView QScrollBar::add-line:horizontal, QTableView QScrollBar::sub-line:horizontal {
    background: #8f8f91;
    border: none;
    border-radius: 3px;
    height: 14px;
    width: 14px;
    subcontrol-origin: margin;
}

/* Scrollbar buttons hover */
QTableView QScrollBar::sub-line:vertical:hover, QTableView QScrollBar::add-line:horizontal:hover, QTableView QScrollBar::sub-line:horizontal:hover {
    background: #6f6f71;
}

/* Scrollbar page areas transparent */
QTableView QScrollBar::add-page:vertical, QTableView QScrollBar::sub-page:vertical,
QTableView QScrollBar::add-page:horizontal, QTableView QScrollBar::sub-page:horizontal {
    background: transparent;
}
