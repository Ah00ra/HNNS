      <property name="styleSheet">
       <string notr="true">/* GroupBox styling */
QGroupBox {
    border: 2px solid navy;
    border-radius: 6px;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 #f6f7fa, stop:1 #dadbde);
    font: 24pt &quot;Segoe UI&quot;;
    padding: 5px;
    color: black;
}

/* GroupBox title styling */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding-left: 0px;
    padding-right: 5px;
    padding-top: 0px;
    margin-top: -22px;
    color: navy;
    font-weight: bold;
}

/* Make QScrollArea (only within GroupBox) have a white background with correct border */
QGroupBox QScrollArea {
    background-color: white !important;
    border: 2px solid #8f8f91;
    border-radius: 6px;
}

/* Make QScrollArea's viewport white as well */
QGroupBox QScrollArea QWidget {
    background-color: white !important;
}

/* Scrollbar styling inside QScrollArea in QGroupBox */
QGroupBox QScrollArea QScrollBar:vertical, QGroupBox QScrollArea QScrollBar:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 #f6f7fa, stop:1 #dadbde);
    border: 2px solid #8f8f91;
    border-radius: 6px;
    width: 14px;
    height: 14px;
    margin: 2px 0 2px 0;
}

/* Remove bottom button of vertical scrollbar */
QGroupBox QScrollArea QScrollBar::add-line:vertical {
    height: 0px;
    width: 0px;
    border: none;
    background: none;
    margin: 0px;
    padding: 0px;
}

/* Scrollbar handle */
QGroupBox QScrollArea QScrollBar::handle:vertical, QGroupBox QScrollArea QScrollBar::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 #8f8f91, stop:1 #6f6f71);
    min-height: 20px;
    border-radius: 6px;
    border: 1px solid #6f6f71;
}

QGroupBox QScrollArea QScrollBar::handle:vertical:hover, QGroupBox QScrollArea QScrollBar::handle:horizontal:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 #6f6f71, stop:1 #4f4f51);
    border: 1px solid #4f4f51;
}

/* Scrollbar sub-line buttons */
QGroupBox QScrollArea QScrollBar::sub-line:vertical, QGroupBox QScrollArea QScrollBar::add-line:horizontal, QGroupBox QScrollArea QScrollBar::sub-line:horizontal {
    background: #8f8f91;
    border: none;
    border-radius: 3px;
    height: 14px;
    width: 14px;
    subcontrol-origin: margin;
}

QGroupBox QScrollArea QScrollBar::sub-line:vertical:hover, QGroupBox QScrollArea QScrollBar::add-line:horizontal:hover, QGroupBox QScrollArea QScrollBar::sub-line:horizontal:hover {
    background: #6f6f71;
}

/* Transparent page areas */
QGroupBox QScrollArea QScrollBar::add-page:vertical, QGroupBox QScrollArea QScrollBar::sub-page:vertical,
QGroupBox QScrollArea QScrollBar::add-page:horizontal, QGroupBox QScrollArea QScrollBar::sub-page:horizontal {
    background: transparent;
}
